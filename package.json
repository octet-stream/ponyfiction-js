{
  "name": "@twi-project/server",
  "version": "0.2.0",
  "codename": "Testing, Testing, 1, 2, 3",
  "description": "Fanfiction library engine",
  "author": "Nick K.",
  "license": "MIT",
  "repository": "git@github.com:octet-stream/twi.git",
  "bugs": "https://github.com/octet-stream/twi/issues",
  "main": "bin/twi",
  "bin": {
    "twi": "bin/twi"
  },
  "scripts": {
    "make": "NODE_ENV=production bin/cake make",
    "watch": "NODE_ENV=development bin/cake watch",
    "m": "yarn make",
    "w": "yarn watch",
    "make:spec": "node_modules/.bin/babel src -s -d .",
    "cleanup": "node_modules/.bin/rimraf cli core database graphql route setup test",
    "lint": "node_modules/.bin/eslint bin build src migration",
    "test": "node_modules/.bin/ava",
    "coverage": "node_modules/.bin/nyc yarn test",
    "codecov": "node_modules/.bin/codecov",
    "report": "yarn make:spec && yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test && yarn cleanup && yarn make",
    "report:ci": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "ci": "yarn make:spec && yarn coverage && yarn report:ci && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn report"
  },
  "ava": {
    "files": [
      "./test/unit/**/*.js",
      "!./test/helper/**/*.js"
    ]
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "dependencies": {
