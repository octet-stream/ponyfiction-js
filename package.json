{
  "name": "@twi-project/server",
  "version": "0.2.0",
  "codename": "Testing, Testing, 1, 2, 3",
  "description": "Fanfiction library engine",
  "author": "Nick K.",
  "license": "MIT",
  "repository": "twi-project/twi-server",
  "bugs": "https://github.com/twi-project/twi-server/issues",
  "main": "bin/twi",
  "bin": {
    "twi": "bin/twi"
  },
  "scripts": {
    "make": "NODE_ENV=production bin/cake make",
    "watch": "NODE_ENV=development bin/cake watch",
    "m": "yarn make",
    "w": "yarn watch",
    "make:spec": "node_modules/.bin/babel src -s -d .",
    "cleanup": "node_modules/.bin/rimraf cli core database graphql route setup test",
    "cleanup:all": "yarn cleanup && node_modules/.bin/rimraf .nyc_output coverage",
    "lint": "node_modules/.bin/eslint bin build src migration",
    "test": "node_modules/.bin/ava && node ./test/helper/database/clear",
    "t": "yarn test",
    "coverage": "node_modules/.bin/nyc yarn test",
    "codecov": "node_modules/.bin/codecov",
    "report": "yarn make:spec && yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test && yarn cleanup && yarn make",
    "report:ci": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "ci": "yarn make:spec && yarn coverage && yarn report:ci && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn report"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "test/**/*.js",
      "!test/**/__hook__/**",
      "!test/**/__helper__/**",
      "!test/helper/**"
    ]
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "dependencies": {
    "@octetstream/invariant": "1.0.2",
    "@octetstream/promisify": "1.0.3",
    "bcryptjs": "2.4.3",
    "camelcase": "4.1.0",
    "chalk": "2.1.0",
    "colors": "1.1.2",
    "commander": "2.11.0",
    "deep-freeze": "0.0.1",
    "elasticsearch": "13.3.1",
    "figures": "2.0.0",
    "graphql": "^0.11.2",
    "graphql-custom-types": "1.1.0",
    "graphql-iso-date": "3.3.0",
    "graphql-server-koa": "1.1.2",
    "graphql-subscriptions": "0.4.4",
    "http2": "3.3.6",
    "jsonwebtoken": "7.4.3",
    "junk": "2.1.0",
    "keygrip": "1.0.2",
    "koa": "2.3.0",
    "koa-bodyparser": "4.2.0",
    "koa-compress": "2.0.0",
    "koa-convert": "1.2.0",
    "koa-favicon": "2.0.0",
    "koa-jwt": "3.2.2",
    "koa-passport": "3.0.0",
    "koa-router": "7.2.1",
    "koa-static": "4.0.1",
    "limax": "1.5.0",
    "lodash": "4.17.4",
    "markdown-it": "8.4.0",
    "mkdirp": "0.5.1",
    "moment": "2.18.1",
    "mongoose": "^4.11.10",
    "ms": "2.0.0",
    "nanoid": "0.2.0",
    "node-yaml": "3.1.1",
    "nodemailer": "4.1.0",
    "object-path": "0.11.4",
    "ora": "1.3.0",
    "parasprite": "^0.6.1",
    "passport": "0.4.0",
    "path-to-regexp": "2.0.0",
    "promise-fs": "1.2.3",
    "pug": "2.0.0-rc.3",
    "redis": "2.8.0",
    "redis-commands": "1.3.1",
    "require-dir": "0.3.2",
    "sprintf-js": "1.1.1",
    "stack-utils": "1.0.1",
    "subscriptions-transport-ws": "0.8.2",
    "then-busboy": "^2.0.0-beta.4",
    "ua-parser-js": "0.7.14",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "1.0.1",
    "ava": "0.22.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-async-generator-functions": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "chokidar": "1.7.0",
    "codecov": "2.3.0",
    "convert-source-map": "1.5.0",
    "eslint": "3.19.x",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-ava": "4.2.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-prettier": "2.2.0",
    "faker": "4.1.0",
    "globby": "6.1.0",
    "gulp-plumber": "1.1.0",
    "gulp-util": "3.0.8",
    "husky": "0.14.3",
    "inquirer": "3.2.3",
    "nyc": "^11.2.0",
    "prettier": "1.6.1",
    "proxyquire": "1.8.0",
    "rimraf": "2.6.1",
    "sinon": "3.2.1",
