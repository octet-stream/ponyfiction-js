    "promise-fs": "1.2.3",
    "pug": "2.0.0-beta11",
    "redis": "2.6.3",
    "redis-commands": "1.3.0",
    "require-dir": "0.3.1",
    "rethinkdb": "2.3.3",
    "sequelize": "3.30.1",
    "shortid": "2.2.6",
