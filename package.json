{
  "name": "twi",
  "version": "0.1.0",
  "description": "Fanfiction library engine for Node.js",
  "author": "Nick K. <nick.kruchinin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/octet-stream/twi-js.git"
  },
  "bugs": {
    "url": "https://github.com/octet-stream/twi-js/issues"
  },
  "main": "./bin/twi",
  "bin": {
    "twi": "bin/twi"
  },
  "scripts": {
    "setup": "bin/twi setup",
    "twi": "bin/twi run",
    "start": "npm run setup && npm run run",
    "cake:make": "./node_modules/coffee-script/bin/cake make",
    "cm": "npm run cake:make",
    "cake:watch": "./node_modules/coffee-script/bin/cake watch",
    "cw": "npm run cake:watch",
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "gulp:make": "npm run gulp",
    "gm": "npm run gulp",
    "gulp:watch": "./node_modules/gulp/bin/gulp.js watch",
    "gw": "npm run gulp:watch",
    "make": "npm run cake:make && npm run gulp:make",
    "m": "npm run make",
    "watch": "twi watch --stack-trace",
    "w": "npm run watch",
    "dev": "nodemon --force-development",
    "d": "npm run dev",
    "test": "./node_modules/ava/cli.js",
    "test:watch": "./node_modules/ava/cli.js --watch",
    "tw": "npm run test:watch"
  },
  "ava": {
    "require": "./test/helper/browser",
    "files": [
      "./test/unit/**/*.js",
      "!./test/helper/**/*.js"
    ]
  },
