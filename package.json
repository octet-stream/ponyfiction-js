{
  "name": "twi",
  "version": "0.2.0",
  "description": "Fanfiction library engine for Node.js",
  "author": "Nick K. <nick.kruchinin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/octet-stream/twi.git"
  },
  "bugs": {
    "url": "https://github.com/octet-stream/twi/issues"
  },
  "main": "bin/twi",
  "bin": {
    "twi": "bin/twi"
  },
  "scripts": {
    "setup": "./bin/twi setup",
    "twi": "./bin/twi run",
    "start": "npm run setup && npm run run",
    "cake:make": "bin/cake make",
    "c:m": "npm run cake:make",
    "cake:watch": "bin/cake watch",
    "c:w": "npm run cake:watch",
    "gulp": "./node_modules/.bin/gulp",
    "gulp:make": "npm run gulp",
    "g:m": "npm run gulp",
    "gulp:watch": "./node_modules/.bin/gulp watch",
    "g:w": "npm run gulp:watch",
    "make": "./bin/twi make && npm run gulp:make",
    "m": "npm run make",
    "watch": "twi watch --stack-trace",
    "w": "npm run watch",
    "dev": "nodemon -x bin/twi dev --stack-trace",
    "d": "npm run dev",
    "test": "bin/cake make && ./node_modules/ava/cli.js",
    "test:watch": "bin/cake make -s theme/twi/src/frontend -d test/runtime/twi/frontend && bin/cake watch -s theme/twi/src/frontend -d test/runtime/twi/frontend & node_modules/ava/cli.js --watch",
    "t:w": "npm run test:watch",
    "coverage": "./node_modules/.bin/nyc npm run test",
    "report": "npm run coverage && ./node_modules/.bin/nyc report --reporter=html npm run test",
    "report:codecov": "./node_modules/.bin/nyc report --reporter=lcov npm run test",
    "codecov": "./node_modules/.bin/codecov",
    "ci": "npm run coverage && npm run report:codecov && npm run codecov"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helper/browser"
    ],
    "babel": "inherit",
    "files": [
      "test/unit/**/*.js",
      "!./test/helper/**/*.js"
    ]
  },
  "dependencies": {
    "bcryptjs": "2.4.0",
