{
  "name": "@twi-project/api-service",
  "version": "0.2.0",
  "codename": "Testing, Testing, 1, 2, 3",
  "description": "Fanfiction library engine",
  "author": "Nick K.",
  "license": "MIT",
  "repository": "twi-project/twi-server",
  "main": "lib/base/main",
  "engines": {
    "node": ">= 10.13"
  },
  "scripts": {
    "make": "babel src -d . --copy-files --include-dotfiles",
    "watch": "node build",
    "m": "npm run make",
    "w": "npm run watch",
    "test": "npm run make:test && ava && npm run cleanup && npm run make",
    "make:test": "babel src -s -d . --copy-files --include-dotfiles",
    "cleanup": "rimraf lib model db api route test setup",
    "cleanup:all": "npm run cleanup && rimraf .nyc_output coverage",
    "lint": "eslint bin build src migration",
    "staged": "lint-staged",
    "coverage": "npm run cleanup && npm run make:test && nyc npm run test && npm run cleanup && npm run make",
    "report": "npm run cleanup && npm run make:test && nyc npm run test && nyc report -r=html && npm run cleanup && npm run make",
    "ci": "npm run make:test && nyc npm run test && nyc report -r=lcov && codecov"
  },
  "nyc": {
    "extension": [
      ".mjs"
    ],
    "exclude": [
      "test/**",
      ".babelrc.js"
    ]
  },
