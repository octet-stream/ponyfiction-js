{
  "name": "twi",
  "version": "0.2.0",
  "description": "Fanfiction library engine",
  "author": "Nick K.",
  "license": "MIT",
  "repository": "git@github.com:octet-stream/twi.git",
  "bugs": "https://github.com/octet-stream/twi/issues",
  "main": "bin/twi",
  "bin": {
    "twi": "bin/twi"
  },
  "scripts": {
    "setup": "bin/twi setup",
    "twi:api": "./bin/twi --stack-trace --env production run api",
    "twi:api:dev": "nodemon -x ./bin/twi -- --stack-trace --env development run api",
    "twi:static": "./bin/twi --stack-trace --env production run static",
    "twi:static:dev": "nodemon -x ./bin/twi -- --stack-trace --env development run static",
    "t:a": "yarn twi:api",
    "t:a:d": "yarn twi:api:dev",
    "t:s": "yarn twi:static",
    "t:s:d": "yarn twi:static:dev",
    "cake:make": "bin/cake make",
    "c:m": "yarn cake:make",
    "cake:watch": "bin/cake watch",
    "c:w": "yarn cake:watch",
    "gulp": "node_modules/.bin/gulp",
    "gulp:make": "yarn gulp",
    "g:m": "yarn gulp",
    "gulp:watch": "node_modules/.bin/gulp watch",
    "g:w": "yarn gulp:watch",
    "make": "node_modules/.bin/mkdirp runtime && bin/twi make",
    "m": "yarn make",
    "watch": "bin/twi watch --stack-trace",
    "w": "yarn watch",
    "make:frontend": "node_modules/.bin/mkdirp runtime && bin/twi make frontend",
    "m:f": "yarn make:frontend",
    "lint": "node_modules/.bin/eslint bin frontend src test next.config.js gulpfile.js",
    "test": "node_modules/.bin/ava",
    "cleanup": "node_modules/.bin/rimraf .nyc_output coverage runtime server static/**/*.svg static/**/*.css static/**/*.js system",
    "precommit": "yarn make && yarn lint && yarn test",
    "coverage": "node_modules/.bin/nyc yarn test",
    "report": "yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test",
    "report:codecov": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "codecov": "node_modules/.bin/codecov",
    "ci": "yarn make && yarn lint && yarn coverage && yarn report:codecov && yarn codecov && yarn cleanup"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helper/env/browser"
    ],
    "babel": "inherit",
    "files": [
      "./test/unit/**/*.js",
      "!./test/helper/**/*.js"
    ]
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
