    "url": "https://github.com/octet-stream/twi-js/issues"
  },
  "main": "./core/server/index.js",
  "bin": {
    "twi": "bin/twi"
  },
  "scripts": {
    "setup": "bin/twi setup",
    "twi": "bin/twi run",
    "start": "npm run setup && npm run run",
    "test": "./node_modules/ava/cli.js",
    "test:devel": "./node_modules/ava/cli.js --watch",
    "td": "./node_modules/ava/cli.js --watch"
  },
  "ava": {
    "verbose": true,
    "require": "./test/helper/browser",
