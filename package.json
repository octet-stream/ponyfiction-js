    "path-to-regexp": "1.7.0",
    "promise-fs": "1.2.3",
    "pug": "2.0.0-beta11",
    "redis": "2.7.0",
    "redis-commands": "1.3.1",
    "require-dir": "0.3.1",
    "rethinkdb": "2.3.3",
