{
  "plugins": [
    "ava"
  ],
  "extends": [
    "@octetstream",
    "plugin:ava/recommended"
  ],
  "rules": {
    "no-void": 0,
    "ava/no-ignored-test-files": ["error", {
      "files": [
        "src/test/unit/**/*.{js,jsx}"
      ]
    }]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
